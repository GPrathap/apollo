# Nvidia Driver on host is required for this config
subnode_config {
    # Camera node. publish pb with non-zero int
    subnodes {
        id: 3
        name: "PylonCameraProcessSubnode"
        reserve: "device_id:camera_right_side;camera_orientation:right_side;pb_obj:1;pb_ln_msk_:0;"
        type: SUBNODE_IN
    }
    # Camera node. publish pb with non-zero int
    subnodes {
        id: 4
        name: "PylonCameraProcessSubnode"
        reserve: "device_id:camera_left_side;camera_orientation:left_side;pb_obj:1;pb_ln_msk_:0;"
        type: SUBNODE_IN
    }
    # Visualization node with OpenGL
    subnodes {
        id: 41
        name: "TrackVisualizationSubnode"
        reserve: "vis_driven_event_id:1008;camera_event_id:1008;camera_orientation:right_side"
    }
    # Visualization node with OpenGL
        subnodes {
        id: 42
        name: "TrackVisualizationSubnode"
        reserve: "vis_driven_event_id:1009;camera_event_id:1009;camera_orientation:left_side"
    }
}

###################################################################
# Define all edges which link nodes.
edge_config {
		# CameraProcessSubnode -> VisualizationSubnode
    edges {
        id: 108
        from_node: 3
        to_node: 41
        events {
            id: 1008
            name: "camera_visualization1"
        }
    }
    # CameraProcessSubnode -> VisualizationSubnode
    edges {
        id: 109
        from_node: 4
        to_node: 42
        events {
            id: 1009
            name: "camera_visualization2"
            }
        }
}

